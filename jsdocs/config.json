{
  "tags": {
    "allowUnknownTags": true
  },
  "recurseDepth": 10,
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "./jsdocs/jsdocCustom"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./jsdoc",
    "package": "package.json",
    "recurse": true,
    "readme": "README.md",
    "tutorials": "./jsdocs/tutorials"
  },
  "docdash": {
    "sectionOrder": [
      "Tutorials",
      "Classes",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Interfaces"
    ]
  },
  "private": false
}
